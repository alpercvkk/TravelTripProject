@using TravelTripProject.Data.Entities
@using TravelTripProject.Models


@model List<Blog>


<div class="padding">
    <h4>En Popüler 10 Blog</h4>
    <ul class="list list-pad">
        @{int i = 0;}

        @foreach (var item in Model)
        {
            <li class="list-bg"><span> @{i++;} @i.  </span><a href="/Blog/BlogDetail/@item.Id"> @item.Title </a></li>
        }
    </ul>
    <a asp-action="Index" asp-controller="Blog" class="link1">Tüm Blogları Gör</a>
</div>







